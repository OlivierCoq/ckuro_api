{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "body": {
      "type": "blocks"
    },
    "hero_image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "title": {
      "type": "string"
    },
    "blog_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-tag.blog-tag",
      "mappedBy": "blog_posts"
    }
  }
}
